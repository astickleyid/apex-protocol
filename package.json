{
  "name": "apex-protocol",
  "version": "5.1.0",
  "description": "AI-Powered Startup Idea Generation and Analysis Platform",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/src/index.js",
    "client:dev": "vite",
    "build": "npm run build:client",
    "build:client": "vite build",
    "start": "node server/src/index.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.11",
    "vite": "^7.3.0"
  }
}
